<Page
    Width="1920" Height="1200"
    x:Class="LabPaymentApp.Debug"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:LabPaymentApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Width="1920" Height="1200">
        <Button x:Name="Back_Button" Content="戻る" HorizontalAlignment="Left" Margin="91,55,0,0" VerticalAlignment="Top" Height="90" Width="219" FontSize="24" Click="Back_Button_Click"/>
